                        <Offset x="12" y="-30"/>
                    </Anchor>
                </Anchors>
                <ScrollChild>
                    <EditBox parentKey="Input" multiLine="true" letters="25000" autoFocus="false">
                        <Size x="443" y="484"/>
                        <Scripts>
                            <OnCursorChanged function="ScrollingEdit_OnCursorChanged"/>
